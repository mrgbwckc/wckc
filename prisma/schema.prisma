generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url       = env("DATABASE_URL")
}

model Client {
  id            Int      @id @default(autoincrement())
  designer      String?
  firstName     String?
  lastName      String
  street        String?
  city          String?
  province      String?
  zip           String?
  phone1        String?
  phone2        String?
  email1        String?
  email2        String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
